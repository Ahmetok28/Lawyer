@model List<BlogDTO>



@foreach (var item in Model)
{
    <div class="block-21 mb-4 d-flex">
        <a class="blog-img mr-4" style="background-image: url(/judge-master/images/image_1.jpg);"></a>
        <div class="text">
            <h3 class="heading"><a href="/@item.SeoUrl" >@item.BlogTitle</a></h3>
            <div class="meta">
                <div><a><span class="icon-calendar"></span> @item.BlogCreatedDate.ToString("dd/MMMM/yyyy")</a></div><br>
                <div><a href="/@item.AuthorSeoUrl"><span class="icon-person"></span> @item.AuthorFullName</a></div>

            </div>
        </div>
    </div>
}



