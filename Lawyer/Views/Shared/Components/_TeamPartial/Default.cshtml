@using Entities.Concrete
@model List<UserForTeamDTO>

@foreach (var item in Model)
{
   
    <div class="col-sm-6 col-md-4 col-lg-3">
    <div class="thumbnail-variant-2-wrap">
        <div class="thumbnail thumbnail-variant-2">
            <figure class="thumbnail-image">
                    <img src="~/Images/ProfilePhotos/@item.ProfliePhoto" alt="" width="246" height="300" style="width:200px;height: 200px" />
            </figure>
            <div class="thumbnail-inner">
                <div class="link-group"><span class="novi-icon icon icon-xxs icon-primary material-icons-local_phone"></span><a class="link-white" href="tel:@item.PhoneNumber">@item.PhoneNumber</a></div>
                <div class="link-group"><span class="novi-icon icon icon-xxs icon-primary fa-envelope-o"></span><a class="link-white" href="mailto: @item.Mail">@item.Mail</a></div>
            </div>
            <div class="thumbnail-caption">
                    <p class="text-header"><a href="/@item.SeoUrl">@item.FullName</a></p>
                <div class="divider divider-md bg-teak"></div>
                <p class="text-caption">@item.Profession</p>
            </div>
        </div>
    </div>
</div>
}
@if (Model.Count < 3)
{
    <div class="col-sm-6 col-md-4 col-lg-3">
        
    </div>
}

